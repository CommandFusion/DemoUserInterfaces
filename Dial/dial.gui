    <theme type="text" name=".text_r"><![CDATA[ color: #FFFFFF; font-size: 16px; font-family: 'Arial'; text-align: right; vertical-align: middle; font-weight: bold; font-style: none; text-decoration: none; display: table-cell;]]></theme>
